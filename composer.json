{
	"name": "wpextra/connect",
	"type": "library",
	"description": "Plugin to write WordPress themes w Object-Oriented Code and the Twig Template Engine",
	"keywords": [
		"timber",
		"twig",
		"themes",
		"templating"
	],
	"require": {
		"php": ">=5.3.0|7.*",
		"google/apiclient": "^2.0",
		"guzzlehttp/guzzle": "^6.3@dev"
	},
	"require-dev": {
		"phpunit/phpunit": "5.7.16"
	},
	"support": {
		"issues": "https://github.com/wpextra/connect/issues",
		"source": "https://github.com/wpextra/connect",
		"docs": "https://connect.github.io/docs/"
	},
	"homepage": "http://wpextra.co",
	"authors": [
		{
			"name": "WP EXTRA",
			"email": "developer@wpextra.co"
		}
	],
	"autoload":          {
        "psr-4":    {
            "Bridge\\Connect\\": "src/"
        }
    },
    "autoload-dev":      {
        "psr-4": {
            "Bridge\\Connect\\Tests\\": "tests/src/"
        }
    },
	"minimum-stability": "dev"
}
